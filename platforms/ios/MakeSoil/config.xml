<?xml version='1.0' encoding='utf-8'?>
<widget id="org.staging.makesoil.app" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="CDVWebViewEngine">
        <param name="ios-package" value="CDVWebViewEngine" />
    </feature>
    <feature name="LaunchScreen">
        <param name="ios-package" value="CDVLaunchScreen" />
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="PushNotification">
        <param name="ios-package" value="PushNotification" />
    </feature>
    <feature name="CustomConfigParameters">
        <param name="ios-package" value="CustomConfigParameters" />
    </feature>
    <feature name="Badge">
        <param name="ios-package" value="APPBadge" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="NativeStorage">
        <param name="ios-package" value="NativeStorage" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVWKInAppBrowser" />
        <param name="onload" value="true" />
    </feature>
    <name>MakeSoil</name>
    <description>MakeSoil Staging</description>
    <author email="dev@makesoil.org" href="https://makesoil.org">
        MakeSoil Dev Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <access origin="about:*" />
    <allow-navigation href="about:" />
    <access origin="https://makesoil2-staging.herokuapp.com" />
    <allow-navigation href="makesoil2-staging.herokuapp.com/*" />
    <allow-navigation href="https://www.youtube.com/*" />
    <allow-navigation href="https://www.dafdirect.org/*" />
    <allow-navigation href="https://donorbox.org/*" />
    <splash src="res/staging/ios/Default@2x~universal~anyany.png" />
    <edit-config file="*-Info.plist" mode="merge" target="ITSAppUsesNonExemptEncryption">
        <false />
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
        <string>MakeSoil only uses your location to match you with nearby soil sites.</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationAlwaysAndWhenInUseUsageDescription">
        <string>MakeSoil only uses your location to match you with nearby soil sites.</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationAlwaysUsageDescription">
        <string>MakeSoil only uses your location to match you with nearby soil sites.</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
        <string>MakeSoil only uses your camera to enable taking and uploading photos.</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>MakeSoil only uses your photo library to allow selection and upload of photos.</string>
    </edit-config>
    <icon height="180" src="res/staging/ios/icon-60@3x.png" width="180" />
    <icon height="60" src="res/staging/ios/icon-60.png" width="60" />
    <icon height="120" src="res/staging/ios/icon-60@2x.png" width="120" />
    <icon height="76" src="res/staging/ios/icon-76.png" width="76" />
    <icon height="152" src="res/staging/ios/icon-76@2x.png" width="152" />
    <icon height="20" src="res/staging/ios/icon-20.png" width="20" />
    <icon height="40" src="res/staging/ios/icon-20@2x.png" width="40" />
    <icon height="40" src="res/staging/ios/icon-40.png" width="40" />
    <icon height="80" src="res/staging/ios/icon-40@2x.png" width="80" />
    <icon height="120" src="res/staging/ios/icon-40@3x.png" width="120" />
    <icon height="57" src="res/staging/ios/icon.png" width="57" />
    <icon height="114" src="res/staging/ios/icon@2x.png" width="114" />
    <icon height="72" src="res/staging/ios/icon-72.png" width="72" />
    <icon height="144" src="res/staging/ios/icon-72@2x.png" width="144" />
    <icon height="167" src="res/staging/ios/icon-167.png" width="167" />
    <icon height="29" src="res/staging/ios/icon-29.png" width="29" />
    <icon height="58" src="res/staging/ios/icon-29@2x.png" width="58" />
    <icon height="87" src="res/staging/ios/icon-29@3x.png" width="87" />
    <icon height="50" src="res/staging/ios/icon-50.png" width="50" />
    <icon height="100" src="res/staging/ios/icon-50@2x.png" width="100" />
    <icon height="1024" src="res/staging/ios/icon-1024.png" width="1024" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaTypesRequiringUserActionForPlayback" value="none" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="android-targetSdkVersion" value="32" />
    <preference name="GradlePluginKotlinVersion" value="1.6.20" />
    <preference name="AndroidXEnabled" value="false" />
    <preference name="GradlePluginGoogleServicesEnabled" value="false" />
    <preference name="AppendUserAgent" value="MakeSoil" />
    <preference name="targetDomain" value="https://makesoil2-staging.herokuapp.com" />
</widget>
